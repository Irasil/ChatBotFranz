<Window x:Class="FranzBot.win2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FranzBot"
        mc:Ignorable="d"
        Title="Keyword Liste" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="73*"/>
            <RowDefinition Height="14*"/>
        </Grid.RowDefinitions>
        <Button x:Name="button" Content="Einfügen" HorizontalAlignment="Left" Margin="670,28,0,0" Grid.Row="1" VerticalAlignment="Top" Height="27" Width="97" Click="button_Click"/>
        <Label x:Name="label" Content="Keyword einfügen" HorizontalAlignment="Left" Margin="131,93,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Antwort einfügen" HorizontalAlignment="Left" Margin="385,93,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tb_key" HorizontalAlignment="Left" Margin="131,124,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tb_answer" HorizontalAlignment="Left" Margin="385,124,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
