<Window x:Class="FranzBot.Key_Eingabe"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FranzBot"
        mc:Ignorable="d"
        Title="Keyword Liste" Height="302" Width="452">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="209*"/>
            <RowDefinition Height="62*"/>
        </Grid.RowDefinitions>
        <Button x:Name="button" Content="Einfügen" HorizontalAlignment="Left" Margin="308,13,0,0" VerticalAlignment="Top" Height="27" Width="97" Click="button_Click" Grid.Row="1"/>
        <Label x:Name="label" Content="Keyword einfügen" HorizontalAlignment="Left" Margin="67,93,0,0" VerticalAlignment="Top" Height="26" Width="106"/>
        <Label x:Name="label1" Content="Antwort einfügen" HorizontalAlignment="Left" Margin="279,93,0,0" VerticalAlignment="Top" Height="26" Width="106"/>
        <TextBox x:Name="tb_key" HorizontalAlignment="Left" Margin="60,124,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Height="18"/>
        <TextBox x:Name="tb_answer" HorizontalAlignment="Left" Margin="272,124,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Height="18"/>

    </Grid>
</Window>
